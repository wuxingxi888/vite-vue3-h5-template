{
  "name": "vite-vue3-h5-template",
  "version": "1.0.0",
  "author": "wuXingXi",
  "bugs": {
    "url": "https://github.com/wuxingxi888/vite-vue3-h5-template/issues"
  },
  "homepage": "https://github.com/wuxingxi888/vite-vue3-h5-template#readme",
  "scripts": {
    "dev": "vite --mode development",
    "prod": "vite --mode production",
    "build:test": "vue-tsc --noEmit && vite build --mode test && esno ./src/utils/build.ts",
    "build:prod": "vue-tsc --noEmit && vite build --mode production && esno ./src/utils/build.ts",
    "build": "vue-tsc --noEmit && vite build",
    "clean:cache": "rm -rf node_modules && rm -rf .eslintcache && pnpm install",
    "preview": "pnpm run build:prod && vite preview --port 5050",
    "preview:dist": "vite preview",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "lint:lint-staged": "lint-staged",
    "preinstall": "npx only-allow pnpm"
  },
  "dependencies": {
    "@vueuse/core": "^7.5.3",
    "axios": "^0.24.0",
    "crypto-js": "^4.1.1",
    "json-bigint": "^1.0.0",
    "mitt": "^3.0.0",
    "pinia": "^2.0.9",
    "pinia-plugin-persistedstate": "^1.0.3",
    "vant": "^4.0.10",
    "vue": "^3.2.26",
    "vue-router": "^4.0.12"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.1.0",
    "@types/crypto-js": "^4.1.1",
    "@types/json-bigint": "^1.0.1",
    "@types/node": "^16.11.17",
    "@vitejs/plugin-legacy": "^3.0.1",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vitejs/plugin-vue-jsx": "^3.0.0",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.2",
    "autoprefixer": "^10.4.2",
    "consola": "^2.15.3",
    "eslint": "^8.5.0",
    "eslint-plugin-vue": "^9.9.0",
    "esno": "^0.16.3",
    "lint-staged": "^12.1.7",
    "postcss": "^8.1.0",
    "postcss-px-to-viewport-8-plugin": "1.1.7",
    "rollup-plugin-visualizer": "^5.9.0",
    "prettier": "^2.5.1",
    "sass": "^1.52.3",
    "terser": "^5.16.1",
    "typescript": "~4.7.3",
    "unplugin-auto-import": "^0.12.2",
    "unplugin-vue-components": "^0.22.12",
    "vite": "^4.0.4",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-imagemin": "^0.6.1",
    "vite-plugin-progress": "^0.0.6",
    "vite-plugin-style-import": "^2.0.0",
    "vue-global-api": "^0.4.1",
    "vue-tsc": "^0.29.8"
  },
  "peerDependencies": {
    "postcss": "^8.1.0"
  },
  "resolutions": {
    "bin-wrapper": "npm:bin-wrapper-china"
  }
