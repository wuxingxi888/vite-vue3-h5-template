{
    "name": "vite-vue3-h5-template",
    "version": "1.0.0",
    "private": true,
    "type": "module",
    "scripts": {
        "prepare": "husky",
        "dev": "turbo run dev",
        "build": "turbo run build",
        "dev:docs": "turbo run dev --filter=docs",
        "dev:h5": "turbo run dev --filter=h5-template",
        "build:docs": "turbo run build --filter=docs",
        "build:h5": "turbo run build --filter=h5-template",
        "clean": "turbo run clean",
        "type-check": "turbo run type-check",
        "lint": "eslint",
        "lint:fix": "eslint --fix",
        "lint:lint-staged": "lint-staged",
        "format": "prettier . --write",
        "format:check": "prettier . --check",
        "deps:check": "pnpm outdated -r",
        "deps:update": "pnpm update --latest",
        "preinstall": "npx only-allow pnpm"
    },
    "devDependencies": {
        "@commitlint/cli": "^19.8.1",
        "@commitlint/config-conventional": "^19.6.0",
        "@miracle/eslint-config": "*",
        "@miracle/prettier-config": "*",
        "cz-git": "^1.12.0",
        "eslint": "^9.35.0",
        "husky": "^9.1.7",
        "lint-staged": "^16.1.6",
        "prettier": "^3.6.2",
        "rimraf": "^5.0.0",
        "turbo": "^2.5.8",
        "typescript": "^5.9.3"
    },
    "resolutions": {
        "bin-wrapper": "npm:bin-wrapper-china"
    },
    "config": {
        "commitizen": {
            "path": "node_modules/cz-git"
        }
    },
    "packageManager": "pnpm@8.15.6"
}
